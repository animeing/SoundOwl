{
  "installCommand": "bash vercel/install.sh || bash ../vercel/install.sh",
  "buildCommand": "bash vercel/build.sh || bash ../vercel/build.sh",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "server.js": {
      "includeFiles": [
        "server/**",
        "css/**",
        "img/**",
        "js/**",
        "parts/setting.ini",
        "manifest.json",
        "favicon.ico",
        "favicon.svg",
        "favicon_*.png"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
