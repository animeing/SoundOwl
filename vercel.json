{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3",
      "excludeFiles": "{node_modules/**,frontend/**,**/*.wasm,**/*.woff*,**/*.ttf,**/*.eot}"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1"  },
    { "src": "/img/(.*)", "dest": "/img/$1"  },
    { "src": "/sound_create/(.*)", "dest": "/sound_create/$1"  },
    { "src": "/css/(.*)", "dest": "/css/$1"  },
    { "src": "/(.*)", "dest": "/index.php" }
  ]
}
