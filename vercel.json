{
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x",
      "includeFiles": [
        "css/**",
        "js/**",
        "img/**",
        "parts/**",
        "vendor/**",
        "manifest.json",
        "favicon.ico",
        "favicon.svg",
        "favicon_16x16.png",
        "favicon_32x32.png",
        "favicon_64x64.png",
        "favicon_128x128.png",
        "favicon_256x256.png"
      ]
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ]
}
